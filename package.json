{
  "name": "tildejs",
  "version": "0.0.4",
  "description": "A little library full of common helper functions so you don't have to write one from scratch.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "eslint",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asbp/tildejs.git"
  },
  "keywords": [
    "js",
    "helpers"
  ],
  "author": "asbp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asbp/tildejs/issues"
  },
  "homepage": "https://github.com/asbp/tildejs#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
